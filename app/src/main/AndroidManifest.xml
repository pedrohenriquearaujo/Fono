<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unicap.fono">
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.unicap.Activity.PacientesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.unicap.Activity.AtividadesActivity" />
        <activity android:name=".CadastroPaciente" />
        <activity android:name="com.example.unicap.Activity.LicaoActivity" />
        <activity android:name="com.example.unicap.Activity.CadastrarAtividadeActivity" />
        <activity android:name="com.example.unicap.Activity.DetalhesActivity" />
        <activity android:name="com.example.unicap.Activity.ExercicioActivity" />
    </application>

</manifest>